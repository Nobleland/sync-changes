            else:
                send "[ Error SP217x81d ]"
        else if arg 1 is "registery-blackbounded_15GE-43Z7-5914-82V3":
            if {undo_redeem+cqJuhSXt} is false:
                set {undo_redeem+cqJuhSXt} to true
